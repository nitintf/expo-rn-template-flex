  "ignorePaths": [
    "node_modules/**",
    "dist/**",
    "build/**",
    ".git/**",
    "pnpm-lock.yaml",
    "*.log",
    "public/locales/**"
  ],
  "ignoreWords": ["VITE", "API"],
  "overrides": [
    {
      "filename": "**/*.{ts,tsx,js,jsx}",
      "ignoreRegExpList": ["/\\b[A-Z_]+\\b/g", "/\\b0x[0-9a-fA-F]+\\b/g"]
    },
    {
      "filename": "**/*.json",
      "ignoreRegExpList": ["/\"[^\"]*\"/g"]
    }
  ]
}
