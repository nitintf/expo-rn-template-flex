{
  "name": "repsy",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "bunx expo start",
    "android": "bunx expo run:android",
    "ios": "bunx expo run:ios",
    "web": "bunx expo start --web",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build:ios:sim": "eas build --profile development --platform ios --local",
    "build:ios:dev": "eas build --profile development:device --platform ios --local",
    "build:ios:prod": "eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:dev": "eas build --profile development:device --platform android --local",
    "build:android:prod": "eas build --profile production --platform android --local",
    "bundle:web": "bunx expo export --platform web",
    "prebuild:clean": "bunx expo prebuild --clean",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "bunx prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml}\"",
    "format:check": "bunx prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yaml}\"",
    "lint": "bunx eslint . --fix",
    "lint:check": "bunx eslint .",
    "test": "bunx jest",
    "test:watch": "bunx jest --watch",
    "test:maestro": "maestro test .maestro/FavoritePodcast.yaml",
    "doctor": "bunx expo-doctor@latest",
    "xcode": "xed -b ios",
    "serve:web": "bunx server dist",
    "patch": "bunx patch-package",
    "postinstall": "bunx patch-package",
    "clean": "rm -rf node_modules && rm -rf dist && bun cache clean",
    "type-check": "tsc --noEmit",
    "validate": "bun compile && bun format:check && bun lint:check && bun test",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.2.2",
    "@expo/metro-runtime": "~4.0.0",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.0.2",
    "@shopify/flash-list": "1.7.1",
    "@supabase/supabase-js": "^2.47.10",
    "date-fns": "^4.1.0",
    "expo": "~52.0.7",
    "expo-application": "~6.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-crypto": "~14.0.1",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.2",
    "expo-localization": "~16.0.0",
    "expo-router": "~4.0.15",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.10",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "i18next": "^23.14.0",
    "intl-pluralrules": "^2.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-native": "0.76.5",
    "react-native-drawer-layout": "^4.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-controller": "^1.12.7",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/react-native": "^12.5.2",
    "@types/jest": "^29.2.1",
    "@types/react": "~18.3.12",
    "babel-jest": "^29.2.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-reactotron": "^0.1.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.1",
    "lint-staged": "^15.3.0",
    "patch-package": "^8.0.0",
    "postinstall-prepare": "1.0.1",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "reactotron-core-client": "^2.9.4",
    "reactotron-react-js": "^3.3.11",
    "reactotron-react-native": "^5.0.5",
    "reactotron-react-native-mmkv": "^0.2.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
